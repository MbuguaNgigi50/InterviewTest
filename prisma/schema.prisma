generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model UserLogin {
  id String @id @default(cuid())
  email String @unique
  password String
  @@unique([id, email])
}

model Products {
  productId String @id @default(uuid())
  productName String @unique
}

model Plans {
  planId String @id @default(uuid())
  planName String
}